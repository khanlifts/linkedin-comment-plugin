{
  "name": "linkedin-focus-plugin",
  "displayName": "Cyril's LinkedIn FeedFocus",
  "version": "1.0.3",
  "description": "Toggle your Notifications and DMs with one click.",
  "author": "Cyril Khan",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.7.2",
    "@plasmohq/storage": "^1.15.0",
    "@supabase/supabase-js": "^2.79.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "tabs",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "key": "$CRX_KEY"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "options.html"
      ],
      "matches": [
        "<all_urls>"
      ],
      "extension_ids": [
        "$CRX_ID"
      ]
    }
  ]
}
